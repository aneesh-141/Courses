   "source": [
    "fname = input(\"Enter file name: \")\n",
    "fh = open(fname)\n",
    "lst = list()\n",
    "for line in fh:\n",
    "    line = line.rstrip()\n",
    "    line = line.split()\n",
    "    for i in line:\n",
    "        if i not in lst:\n",
    "            lst.append(i)\n",
    "            \n",
    "lst.sort() \n",
    "print(lst)"
   ]
